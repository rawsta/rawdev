{
  "printWidth": 90,
  "tabWidth": 2,
  "singleQuote": true,
  "bracketSpacing": true,
  "quoteProps": "consistent",
  "trailingComma": "none",
  "arrowParens": "avoid",
  "plugins": ["prettier-plugin-jinja-template"],
  "overrides": [
    {
      "files": "*.njk",
      "options": {
        "parser": "jinja-template"
      }
    }
  ]
}
